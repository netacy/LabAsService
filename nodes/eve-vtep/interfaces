auto lo
iface lo inet loopback

auto eth9
iface eth9 inet manual
pre-up ip link set dev eth9 address $(/root/mkMac.sh)

auto AP1
iface AP1 inet manual
pre-up brctl addbr AP1
up ip link set AP1 up
post-down brctl delbr AP1

auto eth0
iface eth0 inet manual
up ip link set eth0 up
up brctl addif AP1 eth0
down brctl delif AP1 eth0
down ip link set eth0 down

auto vxlan1001
iface vxlan1001 inet manual
pre-up ip link add vxlan1001 type vxlan id 1001 dstport 0 nolearning
up ip link set vxlan1001 up
up brctl addif AP1 vxlan1001

