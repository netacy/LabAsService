# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

# The loopback network interface
auto lo
iface lo inet loopback
up echo "1" > /proc/sys/net/ipv4/ip_forward
up echo "1" > /proc/sys/net/ipv4/conf/all/proxy_arp


iface eth0 inet manual
auto pnet0
iface pnet0 inet static
    address 10.108.143.50
    netmask 255.255.0.0
    gateway 10.108.255.254
    dns-nameservers 10.100.100.20 10.100.100.21
    bridge_ports eth0
    bridge_stp off


#+++++++++++++++++++++++++++++++++++++++++++++++
auto vxlan2001
iface vxlan2001 inet manual
pre-up ip link add vxlan2001 type vxlan id 2001 remote 10.108.142.1 dstport 4789 dev eth0
post-up bridge fdb append 00:00:00:00:00:00 dev vxlan2001 dst 10.108.142.1

auto vxlan1001
iface vxlan1001 inet manual
pre-up ip link add vxlan1001 type vxlan id 1001 dstport 0 nolearning

auto br2001
iface br2001 inet manual
bridge_ports vxlan2001 vxlan1001
bridge_stp off
